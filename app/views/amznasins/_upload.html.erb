      <%= form_for :photofeed, :url => nonuser_upload_path, :html => {:class => "form-group"} do |f| %>
        <p> Want to upload a photo ? </p>
        <%= f.label "Description", class:"control-label" %>
        <%= f.text_field :asin, :value => params[:asin], class:"hidden" %>
        <%= f.text_field :description, class: "form-control"%>
        <span class="btn btn-info btn-outline btn-file">
          <label for="file">Browse</label><%= f.file_field :photo, :accept => "image/*" %>
        </span>
        <%= f.submit "Upload Photo", class: "btn btn-primary" %>
      <% end %>